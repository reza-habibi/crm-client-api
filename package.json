{
  "name": "client-api",
  "version": "1.0.0",
  "description": "Api for crm client side app",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "*.test.js",
      "fixtures/*"
    ],
    "execMap": {
      "rb": "ruby",
      "pde": "processing --sketch={{pwd}} --run"
    }
  },
  "author": "RezHabibi",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.3",
    "redis": "^3.1.2"
  }
}
